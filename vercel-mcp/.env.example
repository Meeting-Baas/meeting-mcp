# Redis URL for session management
REDIS_URL=redis://username:password@host:port

# Meeting BaaS API key
MEETING_BAAS_API_KEY=your-api-key-here

# Environment (prod, preprod, gmeetbot)
MEETING_BAAS_ENV=prod

# API key for client authentication (optional)
# MCP_API_KEY=your-client-auth-key-here

# Whether to require API key authentication
REQUIRE_API_KEY=false

# Server name and version
MCP_SERVER_NAME=Meeting BaaS MCP
MCP_SERVER_VERSION=1.0.0 